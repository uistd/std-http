{
  "name": "ffan/std-http",
  "description": "FFan http components",
  "keywords": [
    "ffan",
    "php",
    "http"
  ],
  "type": "composer",
  "authors": [
    {
      "name": "huangshunzhao",
      "email": "huangshunzhao@wanda.cn",
      "role": "Founder"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "ffan/std-common": "2.*",
    "ffan/std-logger": "1.*",
    "ffan/std-async": "1.*",
    "ffan/std-console": "1.*"
  },
  "autoload": {
    "psr-4": {
      "FFan\\Std\\Http\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "archive": {
    "exclude": [
      "test",
      ".gitignore"
    ]
  },
  "config": {
    "secure-http": false
  }
}